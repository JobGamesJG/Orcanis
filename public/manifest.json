{
    "manifest_version": 3,
    "name": "Orcanis",
    "description": "Variantion of tools made by JobK",
    "version": "1.2.0",
    "icons": {
        "512": "./icons/icon-512.png"
    },
    "permissions": ["tabs", "storage"],
    "host_permissions": ["<all_urls>"],
    "action": {
        "default_popup": "popup.html"
    },
    "content_scripts": [
        {
            "matches": ["https://mijn.lisahockey.nl/*"],
            "js": ["lisahockey.js"]
        }
    ]
}
